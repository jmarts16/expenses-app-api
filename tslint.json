{
  "extends": [
    "tslint:recommended"
  ],
  "rulesDirectory": ["node_modules/tslint-eslint-rules/dist/rules"],
  "rules": {
    "interface-name": [false],
    "no-console": [false],
    "no-default-export": true,
    "semicolon": [true, "never"],
    "triple-equals": [true, "allow-undefined-check"]
  }
}